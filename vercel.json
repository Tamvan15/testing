{
    "builds": [
        {
            "src": "core/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "source": "/(.*)",
            "destination": "core/wsgi.py"
        },
        {
            "source": "/static/(.*)",
            "destination": "/staticfiles/$1"
        }
    ]
}